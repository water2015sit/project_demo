<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.runzhi.workplacedemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_work_24px"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_work_24px"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".landingscreen.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".notification.NotificationDetailActivity" />
        <activity android:name=".internalfiles.InternalFilesActivity" />
        <activity android:name=".settings.SettingsActivity" />
        <activity android:name=".webbrowser.WebBrowserActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
